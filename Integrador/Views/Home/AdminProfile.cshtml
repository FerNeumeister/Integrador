@model Integrador.Models.Administrador

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron" style="background-color: indigo; color: whitesmoke; padding-bottom: 15px">
    <h2>Perfil administrador:</h2>
</div>

<div>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            <label>Id de Sistema</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdSistema)
        </dd>

        <dt>
            <label>Nombre</label>
        </dt>

        <dd>
            @Model.GetFullName()
        </dd>

        <dt>
            <label>Domicilio</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Domicilio)
        </dd>

        <dt>
            <label>Fecha de alta</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FechaAlta)
        </dd>
    </dl>
</div>
