@model IEnumerable<Integrador.Models.DispositivoInteligente>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreGenerico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Consumo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsoMensualMax)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsoMensualMin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Encendido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModoAhorroDeEnergia)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreGenerico)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Consumo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsoMensualMax)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsoMensualMin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Encendido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModoAhorroDeEnergia)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
